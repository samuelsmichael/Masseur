<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@drawable/masseur_background_main"
    >
    <TextView 
        android:id="@+id/tvManagePrivatePicturesHeading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/TextMasseurHomePageValue"
        android:layout_alignParentTop="true"
        android:layout_marginTop="15dip"
        android:text="Manage Private Photos"
        android:textSize="18sp"
        android:layout_marginBottom="7sp"
        android:layout_centerHorizontal="true"
		                />
    <TableLayout
		    android:id="@+id/tblManagePrivatePictures"
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginTop="10dp"       
		    android:layout_below="@+id/tvManagePrivatePicturesHeading" 
		    android:weightSum="2"
		    >
        <TableRow
            android:id="@+id/trPrivatePicturesRow1"
            android:layout_weight="1"
            >
            <LinearLayout
                android:layout_width="385dp"
                android:layout_height="385dp"
                android:orientation="horizontal"
                android:id="@+id/llrow1"
                >
                
            </LinearLayout>
		</TableRow>
        <TableRow
            android:id="@+id/trPrivatePicturesRow2"
            android:layout_weight="1"
            >
            <LinearLayout
                android:layout_width="385dp"
                android:layout_height="385dp"
                android:orientation="horizontal"
                android:id="@+id/llrow2"
                >
                
            </LinearLayout>
        </TableRow>
        </TableLayout>
   	<Button
        style="@style/ButtonMasseur"
       	android:layout_width="wrap_content"
       	android:layout_height="wrap_content"
        android:id="@+id/btnManagePrivatePhotosContinue"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Continue" />
</RelativeLayout>
